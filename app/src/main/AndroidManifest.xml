<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ikhsanhdyt.suitgamenextlevel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SuitGameNextLevel">
        <activity android:name=".ui.game.GameActivity"
            android:exported="false"></activity>
        <activity
            android:name="com.ikhsanhdyt.suitgamenextlevel.ui.landingPage.LandingPageActivity"
            android:label="My Landing Page"/>
        <activity
            android:name=".ui.menu.MenuActivity"
            android:exported="false" />
        <activity
            android:name="com.ikhsanhdyt.suitgamenextlevel.ui.splash.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>